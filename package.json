{
  "name": "yumemi-frontend-codetest",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "fix": "eslint --fix --ext .js,.ts,.vue src netlify; stylelint --fix \"**/*.vue\" -f verbose; prettier --write .",
    "lint": "eslint --max-warnings 0 --ext .js,.ts,.vue src netlify && stylelint --fix \"**/*.vue\" -f verbose && prettier --check .",
    "typeck": "vue-tsc --noEmit",
    "test": "vitest run",
    "cy:test": "start-server-and-test 8888 cy:run",
    "cy:run": "cypress run",
    "start": "ntl dev",
    "postinstall": "patch-package"
  },
  "pre-commit": [
    "lint",
    "typeck",
    "test"
  ],
  "dependencies": {
    "@vue/test-utils": "^2.0.0-rc.18",
    "cypress": "^9.2.1",
    "echarts": "^5.2.2",
    "got": "^12.0.1",
    "ky": "^0.28.7",
    "lodash": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "vue": "^3.2.25",
    "vue-echarts": "^6.0.1"
  },
  "devDependencies": {
    "@netlify/functions": "^0.10.0",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.2.0",
    "happy-dom": "^2.25.2",
    "netlify-cli": "^8.9.2",
    "patch-package": "^6.4.7",
    "postcss-html": "^1.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.1.18",
    "vue-tsc": "^0.30.0"
  },
  "engines": {
    "node": "<17"
  }
}
